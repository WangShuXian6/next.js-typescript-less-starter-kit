{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "jsx": "preserve",
    "sourceMap": false,
    "moduleResolution": "node",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl":".",
    "paths":{
      "@/components/*": ["./components/*"],
      "@/assets/*": ["./assets/*"],
      "@/constants/*": ["./constants/*"],
      "@/lib/*": ["./lib/*"],
      "@/mock/*": ["./mock/*"],
      "@/pages/*": ["./pages/*"],
      "@/services/*": ["./services/*"],
    }
  },
  "exclude": [
    "out",
    ".next"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
